{
  "defaultSeverity": "warning",
  "extends": ["tslint:recommended", "tslint-plugin-prettier", "tslint-config-prettier"],
  "rules": {
    "ordered-imports": false,
    "no-console": false,
    "no-submodule-imports": false,
    "object-literal-sort-keys": false,
    "prettier": true,
    "prefer-template": true
  },
  "rulesDirectory": []
}
